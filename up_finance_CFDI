	IF @TipoConsulta = 53
	BEGIN
		select @TipoDoc = TipoDoc, @DocEntry = DocEntry from [HNT-Files].dbo.File_CFDI_HNT_TRZ where nIdArchivo = @nIdArchivo
		--select  * from SBOCTRZ.dbo.OPCH where U_EDOCNUM = 'CC7FAC4A-103E-11EE-887C-D53E0AE5A086'
		--IF OPCH FACTURA 
		-- IF ORPC NC
		-- IF OVPN PAGO
		--U_EDDOCNUM = NULL
		IF @TipoDoc LIKE '%Factura%'
		BEGIN
			UPDATE SBOCTRZ.dbo.OPCH SET U_Edocnum = NULL WHERE DocEntry = @DocEntry
		END
		IF @TipoDoc LIKE '%Credito%'
		BEGIN
			UPDATE SBOCTRZ.dbo.ORPC SET U_Edocnum = NULL WHERE DocEntry = @DocEntry
		END
		IF @TipoDoc LIKE '%Pago%'
		BEGIN
			UPDATE SBOCTRZ.dbo.OVPM SET U_Edocnum = NULL WHERE DocEntry = @DocEntry
		END
		
		delete from [HNT-Files].dbo.File_CFDI_HNT_TRZ where nIdArchivo = @nIdArchivo
		select 'Los archivos se han eliminado correctamente' Msj
	END

	IF @TipoConsulta = 54
	BEGIN
		IF EXISTS (SELECT * FROM [finance].[CFDISAT] WHERE MONTH(Fecha) = @Mes AND YEAR(Fecha) = @Anho)
		BEGIN
			SELECT CAST(1 AS BIT)[Response]
		END
		ELSE
		BEGIN
			SELECT  CAST(0 AS BIT)[Response]
		END
	END

	IF @TipoConsulta = 55
	BEGIN
		BEGIN TRAN
		BEGIN TRY
			DELETE FROM [finance].[CFDISAT] WHERE MONTH(Fecha) = @Mes AND YEAR(Fecha) = @Anho
			SELECT @Message = 'Información eliminada correctamente', @Type = 'success';
		COMMIT
		END TRY
		BEGIN CATCH
		ROLLBACK
			RAISERROR('Ocurrió un error al ejecutar la operacíón, intentelo nuevamente', 11, 1);
		END CATCH
	END

	IF @TipoConsulta = 56
	BEGIN
		BEGIN TRAN
		BEGIN TRY
			DELETE FROM [finance].[CFDISAT] WHERE UUID = @UUID		
			INSERT INTO [finance].[CFDISAT] 
			   ([Estatus], [EfectoComprobante], [UUID], [Fecha], [UsoCFDI], [EmisorRFC], [Proveedor], [emisorRegimen], [Conceptos], [Monto],
			   [DocNum],[DocEntry],[DocType],[ObjType],[DocTotal],[DocTotalfc] ,[DocDate],[Razon],[Moneda],[Version], [FormaPago], [MetodoPago], [TipoCambio])
			VALUES 
				(@Estatus, @EfectoComprobante, @UUID, @Fecha, @UsoCFDI, @EmisorRFC, @Proveedor, @RegimenFiscal, @Conceptos,  @Monto,
			   @DocNum, @DocEntry,@DocType,@ObjType, @DocTotal, @DocTotalfc, @DocDate,@Mensaje,@Moneda, 'A', @FormaPago, @MetodoPago, @TipoCambio)

			SELECT @Message = 'Listo', @Type = 'success';
		COMMIT
		END TRY
		BEGIN CATCH
		ROLLBACK
			DECLARE @ErrorMsg VARCHAR(500) = (SELECT ERROR_MESSAGE() AS ERROR);
			RAISERROR(@ErrorMsg, 11, 1);
		END CATCH	
	END

	IF @TipoConsulta = 57
	BEGIN
		IF @Opcion = 1
		BEGIN
			BEGIN TRY
				BEGIN TRANSACTION;
					DELETE FROM [finance].[CFDISATV2] WHERE MONTH(Fecha) = @Mes AND YEAR(Fecha) = @Anho AND Empresa = @Empresa
					SELECT 'Información eliminada correctamente' [Mensaje], 'success' [Tipo];
				COMMIT TRANSACTION;
			END TRY
			BEGIN CATCH
			ROLLBACK TRANSACTION; 
			SELECT 
				ERROR_MESSAGE() AS Mensaje,
				'error' AS Tipo;
			END CATCH
		END

		IF @Opcion = 2
		BEGIN
			BEGIN TRY
				BEGIN TRANSACTION;
					DELETE f
					FROM finance.CFDISATV2 f
					INNER JOIN @TablaCFDISAT t ON f.UUID = t.[Folio Fiscal]
					WHERE f.Empresa = @Empresa

					--DELETE FROM finance.CFDISATV2
					--WHERE UUID IN (SELECT [Folio Fiscal] FROM @TablaCFDISAT) AND Empresa = @Empresa

					INSERT INTO finance.CFDISATV2  
					SELECT @Empresa, Fecha, Total, [Nombre o Razon Social del Emisor], [Folio Fiscal], Moneda, GETDATE()
					, 'A', [Efecto del Comprobante], [Estado del Comprobante], [Uso Cfdi], [RFC Emisor], ConceptosDescripcion, [Forma de Pago]
					, [Metodo de Pago], [Emisor Regimen], Tipo_Cambio FROM  @TablaCFDISAT 
					SELECT 'Se insertaron los datos' Mensaje, 'success' Tipo
				COMMIT TRANSACTION;
			END TRY

			BEGIN CATCH
			ROLLBACK TRANSACTION; 
			SELECT 
				ERROR_MESSAGE() AS MensajeError,
				'error' AS Tipo;
			END CATCH
		END
	END

	IF @TipoConsulta = 58
	BEGIN
		SET @QRY = '
		SELECT 
			*
		FROM (
			SELECT
				A.Estatus
				, ISNULL(A.EfectoComprobante,'''') [Efecto comprobante]
				, A.UUID
				, FORMAT(A.Fecha, ''yyyy-MM-dd'') [Fecha SAT]
				, A.UsoCFDI
				, A.FormaPago
				, A.MetodoPago
				, A.emisorRegimen [Regimen emisor]
				, A.EmisorRFC [RFC emisor]
				, A.Proveedor [Nombre emisor]
				, A.Conceptos
				, A.Moneda
				, A.Monto
				--, A.TipoCambio
				, MAX(C.DocNum) [OC SAP]
				, MAX(B.DocNum) [Factura SAP] 				
				, MAX(D.DocNum) [NC SAP] '
				IF @Empresa = 'SBOCTRZ'
					SET @QRY += ', CASE WHEN (SELECT TOP 1 X.nIdArchivo FROM [HNT-Files].dbo.File_CFDI_HNT_TRZ x WHERE X.UUID COLLATE DATABASE_DEFAULT= A.UUID AND Extension = ''.xml'' ) IS NOT NULL THEN ''CARGADO'' END [XML] '
				ELSE IF @Empresa = 'SBODPJ'
					SET @QRY += ', CASE WHEN (SELECT TOP 1 X.nIdArchivo FROM [HNT-Files].dbo.File_CFDI x WHERE X.UUID COLLATE DATABASE_DEFAULT= A.UUID AND Extension = ''.xml'' ) IS NOT NULL THEN ''CARGADO'' END [XML] '
			SET @QRY += ' FROM finance.[CFDISATV2] A
				LEFT JOIN ' + @Empresa + '.DBO.OPCH B ON A.UUID = B.U_Edocnum AND ISNULL(B.Indicator, '''') <> ''CF'' AND A.EfectoComprobante = ''Ingreso''
				LEFT JOIN ' + @Empresa + '.DBO.OPOR C ON A.UUID = C.U_Edocnum AND ISNULL(C.Indicator, '''') <> ''CF'' AND A.EfectoComprobante = ''Ingreso''
				LEFT JOIN ' + @Empresa + '.DBO.ORPC D ON A.UUID = D.U_Edocnum AND ISNULL(D.Indicator, '''') <> ''CF'' AND A.EfectoComprobante = ''Egreso''
			WHERE 
				A.Empresa = @Empresa
				AND A.Fecha BETWEEN @Desde AND @Hasta
				--AND A.Monto <> 0
				--AND ISNULL(B.U_Edocnum, D.U_Edocnum) IS NULL
			GROUP BY 
				A.Estatus
				, A.EfectoComprobante
				, A.UUID
				, A.Fecha
				, A.UsoCFDI
				, A.EmisorRFC
				, A.Proveedor
				, A.emisorRegimen
				, A.Conceptos
				, A.Monto
				, A.TipoCambio
				, A.FormaPago
				, A.Moneda
				, A.MetodoPago
		)T0
		WHERE
			'
			--PARA CTRZ y DPJ es necesario cargar archivos en halconet, en el resto de empresas solo se valida que exista el registro en SAP
			IF @Empresa IN ('SBOCTRZ', 'SBODPJ') 
				SET @QRY +=' ISNULL([NC SAP], [Factura SAP]) IS NULL OR [XML] IS NULL '
			ELSE
				SET @QRY +='  ISNULL([NC SAP], [Factura SAP]) IS NULL '

			SET @Params = '@Desde DATE, @Hasta DATE, @Empresa NVARCHAR(100)'
		EXEC sp_executesql @QRY, @Params, @Desde, @Hasta, @Empresa
	END

END
