		--	UNION ALL
		--	SELECT 'RFCReceptor'[ColF], 'Receptor,rfc,1'[Nodo], 'RFC Receptor'[Valor] , ''[Factura], ''[XML], ''[Resultado] 
		--	UNION ALL
		--	SELECT 'ValidacionSAT'[ColF], 'ValidacionSAT'[Nodo], 'SAT'[Valor] , '---'[Factura], ''[XML], ''[Resultado] 

		--END
	END

	IF @TipoConsulta = 12
	BEGIN
		IF NOT EXISTS(SELECT * FROM [HNT-Files].dbo.File_CFDI_HNT_TRZ WHERE DocEntry = @DocEntry AND DocNum = @DocNum AND Extension = @Extension AND TipoDoc = @TipoDoc AND nTipoEmpresa = @TipoEmpresa AND Version = @_Version)
		BEGIN
			INSERT INTO [HNT-Files].dbo.File_CFDI_HNT_TRZ (DocEntry, DocNum, DocType, UUID, CardCode, RutaArc, Archivo, Nombre, Extension, TipoDoc, nUsuario, dAlta, nTipoEmpresa, Version, Emisor, Receptor, MetodoPago, dTotal, FormaPago, dFechaPago)
								VALUES(@DocEntry, @DocNum, @DocType, @UUID, @CardCode, @RutaArc, @Archivo, @Nombre, @Extension, @TipoDoc, @User_ID, GETDATE(), @TipoEmpresa, @_Version, @Emisor, @Receptor, @MetodoPago, @Monto, @FormaPago, @FechaPago)

								
		END
		ELSE
		BEGIN
			UPDATE [HNT-Files].dbo.File_CFDI_HNT_TRZ
			SET DocType = @DocType
			,UUID = @UUID
			,CardCode = @CardCode
			,RutaArc = @RutaArc
			, Archivo = @Archivo
			, Nombre = @Nombre
			, Extension = @Extension
			, TipoDoc = @TipoDoc
			, nUsuario = @User_ID
			, dAlta = GETDATE()
			, Emisor = @Emisor
			, Receptor = @Receptor
			, MetodoPago = @MetodoPago
			, dTotal = @Monto
			, FormaPago = @FormaPago
			, dFechaPago = @FechaPago

			WHERE DocEntry = @DocEntry AND DocNum = @DocNum AND Extension = @Extension AND nTipoEmpresa = @TipoEmpresa AND Version = @_Version
		END
	END

	--CONSULTA PARA VERIFICAR SI YA EXISTEN ARCHIVOS CARGADOS PREVIAMENTE A LA FACTURA
	IF @TipoConsulta = 13
	BEGIN
		SELECT 
			nIdArchivo
			, DocEntry
			, DocNum
			, DocType
			, UUID
			, CardCode
			, RutaArc
			, Archivo
			, Nombre
			, Extension
			, Nombre + ' ' + Extension [NombreCompleto]
			, TipoDoc
		FROM 
			[HNT-Files].dbo.File_CFDI_HNT_TRZ 
		WHERE 
			DocEntry = @DocEntry 
			AND CardCode = @CardCode 
			AND TipoDoc = @TipoDoc 
			AND nTipoEmpresa = @TipoEmpresa
			AND Version = @_Version
	END

	----SE ELIMINAN LAS FACTURAS PREVIAMENTE YA CARGADAS
	IF @TipoConsulta = 14
	BEGIN
		DELETE
		[HNT-Files].dbo.File_CFDI_HNT_TRZ 
		WHERE CardCode = @CardCode AND DocEntry = @DocEntry	
			AND nTipoEmpresa = @TipoEmpresa AND Version = @_Version
	END

